{
	"name": "presentation-manager",
	"private": true,
	"version": "1.0.2",
	"description": "BuzzCasting Presentation Manager",
	"type": "module",
	"main": "dist/umd/index.js",
	"module": "dist/es/index.js",
	"unpkg": "./dist/umd/index.js",
	"types": "dist/types/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/presentation-manager.js",
			"require": "./dist/presentation-manager.umd.cjs"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/TouchFlows/presentation-manager.git"
	},
	"scripts": {
