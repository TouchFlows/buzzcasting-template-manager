	"keywords": [
		"grapesjs",
		"plugin"
	],
	"devDependencies": {
		"@types/dom-to-image": "^2.6.4",
		"@types/node": "^20.1.4",
		"grapesjs": "^0.21.1",
		"rollup-plugin-typescript-paths": "^1.4.0",
		"typescript": "^5.0.2",
		"vite": "^4.3.5",
		"vite-plugin-dts": "^2.3.0"
	},
	"author": "Mark Tamis",
	"license": "Unlicensed",
	"dependencies": {
		"dom-to-image": "^2.6.0",
		"ulid": "^2.3.0"
